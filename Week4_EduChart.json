{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "Tertiary education enrollment for Vietnam and peer countries from World Bank API",
  "data": {
    "url": "https://api.worldbank.org/v2/country/VNM;THA;IDN;PHL;MYS/indicator/SE.TER.ENRR?format=json&per_page=1000&date=1990:2025",
    "format": {"property": "[1]"}
  },
  "width": 500,
  "height": 350,
  "mark": {"type": "line", "point": true},
  "title": {
    "text": "Tertiary Education Enrollment",
    "subtitle": "Vietnam and peer countries (% gross). Source: World Bank API"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "",
      "axis": {"grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Tertiary School enrollment (% gross)",
      "axis": {"grid": true}
    },
    "color": {
      "field": "country.value",
      "type": "nominal",
      "legend": {"title": "Country"}
    },
    "tooltip": [
      {"field": "country.value", "type": "nominal", "title": "Country"},
      {"field": "date", "type": "temporal", "title": "Year"},
      {
        "field": "value",
        "type": "quantitative",
        "title": "Enrollment %",
        "format": ".2f"
      }
    ]
  },
  "config": {}
}