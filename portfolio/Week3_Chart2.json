{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 700,
  "height": 400,
  "title": {
    "text": "Rising Immigration Does Not Increase Native Unemployment",
    "subtitle": "Source: Federal Reserve Economic Data (FRED)",
    "fontSize": 18,
    "subtitleFontSize": 12,
    "anchor": "start"
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/nethranatarajan3/nethranatarajan3.github.io/main/portfolio_data/Foreign_Population.csv"
      },
      "transform": [{"calculate": "'Foreign-Born Population'", "as": "metric"}],
      "mark": {"type": "line", "strokeWidth": 3},
      "encoding": {
        "x": {
          "field": "observation_date",
          "type": "temporal",
          "title": "Year",
          "axis": {"format": "%Y"}
        },
        "y": {
          "field": "LNU00073395",
          "type": "quantitative",
          "title": "Foreign-Born Population (thousands)",
          "axis": {"titleColor": "#e74c3c"}
        },
        "color": {
          "field": "metric",
          "type": "nominal",
          "scale": {
            "domain": ["Foreign-Born Population", "Native-Born Unemployment"],
            "range": ["#e74c3c", "#4169E1"]
          },
          "legend": {"title": "Metric"}
        },
        "tooltip": [
          {
            "field": "observation_date",
            "type": "temporal",
            "title": "Date",
            "format": "%Y-%m"
          },
          {"field": "metric", "type": "nominal", "title": "Metric"},
          {
            "field": "LNU00073395",
            "type": "quantitative",
            "title": "Population",
            "format": ",.0f"
          }
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/nethranatarajan3/nethranatarajan3.github.io/main/portfolio_data/Native_Unemployment.csv"
      },
      "transform": [
        {"calculate": "'Native-Born Unemployment'", "as": "metric"}
      ],
      "mark": {"type": "line", "strokeWidth": 3},
      "encoding": {
        "x": {"field": "observation_date", "type": "temporal"},
        "y": {
          "field": "LNU04073413",
          "type": "quantitative",
          "title": "Native-Born Unemployment Rate (%)",
          "axis": {"titleColor": "#4169E1", "orient": "right"}
        },
        "color": {
          "field": "metric",
          "type": "nominal",
          "scale": {
            "domain": ["Foreign-Born Population", "Native-Born Unemployment"],
            "range": ["#e74c3c", "#4169E1"]
          }
        },
        "tooltip": [
          {
            "field": "observation_date",
            "type": "temporal",
            "title": "Date",
            "format": "%Y-%m"
          },
          {"field": "metric", "type": "nominal", "title": "Metric"},
          {
            "field": "LNU04073413",
            "type": "quantitative",
            "title": "Unemployment Rate",
            "format": ".1f"
          }
        ]
      }
    },
    {
      "data": {"values": [{"date": "2008-09-15"}]},
      "mark": {
        "type": "rule",
        "strokeDash": [5, 5],
        "color": "gray",
        "strokeWidth": 2
      },
      "encoding": {"x": {"field": "date", "type": "temporal"}}
    },
    {
      "data": {
        "values": [{"date": "2008-09-15", "label": "2008 Financial Crisis"}]
      },
      "mark": {
        "type": "text",
        "align": "center",
        "baseline": "top",
        "dy": -20,
        "dx": 50,
        "fontSize": 10,
        "color": "black",
        "angle": 0
      },
      "encoding": {
        "x": {"field": "date", "type": "temporal"},
        "y": {"value": 400},
        "text": {"field": "label"}
      }
    },
    {
      "data": {"values": [{"date": "2020-03-11"}]},
      "mark": {
        "type": "rule",
        "strokeDash": [5, 5],
        "color": "gray",
        "strokeWidth": 2
      },
      "encoding": {"x": {"field": "date", "type": "temporal"}}
    },
    {
      "data": {"values": [{"date": "2020-03-11", "label": "COVID-19"}]},
      "mark": {
        "type": "text",
        "align": "center",
        "baseline": "top",
        "dy": -20,
        "dx": 30,
        "fontSize": 10,
        "color": "black",
        "angle": 0
      },
      "encoding": {
        "x": {"field": "date", "type": "temporal"},
        "y": {"value": 400},
        "text": {"field": "label"}
      }
    }
  ],
  "resolve": {"scale": {"y": "independent"}},
  "config": {}
}