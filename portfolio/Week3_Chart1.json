{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 700,
  "height": 400,
  "title": {
    "text": "Native and Foreign-Born Unemployment Show Similar Trajectories",
    "subtitle": "Source: Federal Reserve Economic Data (FRED)",
    "fontSize": 18,
    "subtitleFontSize": 12,
    "anchor": "start"
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/nethranatarajan3/nethranatarajan3.github.io/main/portfolio_data/Foreign_Unemployment.csv"
      },
      "transform": [{"calculate": "'Foreign-Born'", "as": "group"}],
      "mark": {"type": "line", "strokeWidth": 3},
      "encoding": {
        "x": {
          "field": "observation_date",
          "type": "temporal",
          "title": "Year",
          "axis": {"format": "%Y"}
        },
        "y": {
          "field": "LNU04073395",
          "type": "quantitative",
          "title": "Unemployment Rate (%)"
        },
        "color": {
          "field": "group",
          "type": "nominal",
          "scale": {
            "domain": ["Foreign-Born", "Native-Born"],
            "range": ["#e74c3c", "#4169E1"]
          },
          "legend": {"title": "Population Group"}
        },
        "tooltip": [
          {
            "field": "observation_date",
            "type": "temporal",
            "title": "Date",
            "format": "%Y-%m"
          },
          {"field": "group", "type": "nominal", "title": "Group"},
          {
            "field": "LNU04073395",
            "type": "quantitative",
            "title": "Unemployment Rate",
            "format": ".1f"
          }
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/nethranatarajan3/nethranatarajan3.github.io/main/portfolio_data/Native_Unemployment.csv"
      },
      "transform": [{"calculate": "'Native-Born'", "as": "group"}],
      "mark": {"type": "line", "strokeWidth": 3},
      "encoding": {
        "x": {"field": "observation_date", "type": "temporal"},
        "y": {"field": "LNU04073413", "type": "quantitative"},
        "color": {
          "field": "group",
          "type": "nominal",
          "scale": {
            "domain": ["Foreign-Born", "Native-Born"],
            "range": ["#e74c3c", "#4169E1"]
          }
        },
        "tooltip": [
          {
            "field": "observation_date",
            "type": "temporal",
            "title": "Date",
            "format": "%Y-%m"
          },
          {"field": "group", "type": "nominal", "title": "Group"},
          {
            "field": "LNU04073413",
            "type": "quantitative",
            "title": "Unemployment Rate",
            "format": ".1f"
          }
        ]
      }
    }
  ],
  "config": {}
}